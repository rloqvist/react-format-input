{
  "name": "react-format-input",
  "version": "0.1.0",
  "author": "rloqvist",
  "description": "A lightweight wrapper around react-number-format",
  "dependencies": {
    "react-number-format": "^4.4.1"
  },
  "keywords": [
    "react",
    "format",
    "input"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "clean": "rimraf dist node_modules",
