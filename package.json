    "watch": "cross-env NODE_ENV=production babel src --out-dir dist --copy-files --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.25.0",
    "babel-preset-react-app": "^9.0.0",
    "cross-env": "^7.0.0",
    "rimraf": "3.0.1"
  }
}
